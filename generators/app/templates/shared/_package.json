{
    "name": "<%= slugify(app.name) %>",
    "version": "<%= app.version %>",
    "description": "<%= app.desc %>",
    "scripts": {
        "test-server": "jest -c jest/jest.server.config.js",
        "test-client": "jest -c jest/jest.client.config.js",
        "clear": "rm -rf dist/",
        "dev": "ts-node --project tsconfig.server.json server/index.ts",
        "build-next": "cross-env NODE_ENV=production next build",
        "build-ts": "cross-env NODE_ENV=production tsc --project tsconfig.server.json",
        "build": "yarn clear && yarn build-next && yarn build-ts",
        "start": "cross-env NODE_ENV=production node dist/server/server/index.js",
        "pm2": "pm2",
        "pm2-start": "pm2 start npm --name \"<%= slugify(app.name) %>\" -i max -- start",
        "release": "yarn test-client && yarn test-server && ./scripts/release.sh",
        "analyze": "cross-env BUNDLE_ANALYZE=both yarn build"
    },
    "keywords": [],
    "author": "<%= app.author %>",
    "license": "<%= app.license %>",
    "private": <%= app.private %>,
    "dependencies": {
        "@types/classnames": "^2.2.9",
        "@zeit/next-bundle-analyzer": "^0.1.2",
        "@zeit/next-sass": "^1.0.1",
