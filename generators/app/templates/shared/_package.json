        "babel-plugin-styled-components": "^1.10.6",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.3.1",
        "classnames": "^2.2.6",
        "connect-mongo": "^3.0.0",
        "cors": "^2.8.5",
        "cross-env": "^5.2.0",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-device": "^0.4.2",
        "express-session": "^1.16.2",
        "helmet": "^3.19.0",
        "i18next": "^17.0.6",
        "i18next-browser-languagedetector": "^3.0.1",
        "i18next-express-middleware": "^1.8.0",
        "i18next-xhr-backend": "^3.0.0",
        "lodash": "^4.17.14",
        "lru-cache": "^5.1.1",
        "mongoose": "^5.6.5",
        "next": "9.0.2",
        "next-i18next": "^0.47.0",
        "next-redux-wrapper": "^3.0.0-alpha.3",
        "node-sass": "^4.12.0",
        "pm2": "^3.5.1",
        "react": "^16.8.6",
