{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "noEmit": false,
        "module": "commonjs",
        "esModuleInterop": false,
        "isolatedModules": false,
        "allowSyntheticDefaultImports": false,
        "outDir": "dist/server",
        "target": "es2017",
        "lib": ["dom", "es2015", "es2016", "es2017"]
    },
    "include": ["server/**/*.ts"],
    "exclude": [".next", "dist", "tests"]
}
